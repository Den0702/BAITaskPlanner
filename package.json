    "cordova-plugin-crosswalk-webview": "^2.4.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
